    "@types/jspdf": "^1.3.3",
    "cookie": "^1.0.2",
    "jspdf": "^3.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.468.0",
    "next": "^15.2.2",
    "quality-lab-pro": "file:",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "recharts": "^2.15.0",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
