    "prettier-format": "prettier --config .prettierrc --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "commitlint": "commitlint -e --verbose"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.13",
